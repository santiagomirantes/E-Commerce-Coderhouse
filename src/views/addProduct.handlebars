<h1>Add Product</h1>

<form>
    <label for="code">Code</label>
    <input type="number" name="code" id="code" placeholder="e.g. 1291">
    <label for="title">Title</label>
    <input type="text" name="title" id="title" placeholder="e.g. Product one">
    <label for="description">Description</label>
    <input type="text" name="description" id="description" placeholder="e.g. A beautiful nice product">
    <label for="stock">Stock</label>
    <input type="number" name="stock" id="stock" placeholder="e.g. 12">
    <label for="price">Price</label>
    <input type="number" name="price" id="price" placeholder="e.g. 1200">
    <label for="category">Category</label>
    <input type="text" name="category" id="category" placeholder="e.g. Furnace">
    <input type="button" value="Submit">
</form>
<script src="/scripts/addProduct.js"></script>